              for (const el of h6Elements) {
                  if (el.textContent.trim() === 'Desenvolvido por:') {
                      const nextDiv = el.nextElementSibling;
                      if (nextDiv && nextDiv.tagName === 'DIV') {
                          const link = document.createElement('a');
                          link.href = `https://www.nuvemshop.com.br/loja-aplicativos-nuvem/busca?q=huapps`;
                          const newImg = document.createElement('img');
                          newImg.src = img.getAttribute('src');
                          newImg.style.maxHeight = '30px'; // Ajuste de altura
                          nextDiv.innerHTML = '';
                          link.appendChild(newImg)
                          nextDiv.appendChild(link);
                          break; // Para após encontrar o primeiro caso
                      }
                  }
              }
          })(this);
